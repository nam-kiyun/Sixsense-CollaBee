// 로그인 페이지로 이동
scwin.goToLogin = function() {
	window.location.href = "/InsWebApp/websquare/websquare.html?w2xPath=/InsWebApp/ui/login/login.xml";
};

// 회원가입 페이지로 이동  
scwin.goToSignup = function() {
	window.location.href = "/InsWebApp/websquare/websquare.html?w2xPath=/InsWebApp/ui/login/signup.xml";
};

scwin.onpageload = function() {
	// JavaScript로 hover 효과 구현 (WebSquare 환경에서 확실한 방법)
	
	// 모든 요소 찾기 함수
	function addHoverEffect(selector, hoverStyle, originalStyle) {
		const elements = document.querySelectorAll(selector);
		elements.forEach(function(element) {
			element.addEventListener('mouseenter', function() {
				Object.keys(hoverStyle).forEach(function(key) {
					element.style[key] = hoverStyle[key];
				});
			});
			element.addEventListener('mouseleave', function() {
				Object.keys(originalStyle).forEach(function(key) {
					element.style[key] = originalStyle[key];
				});
			});
		});
	}
	
	setTimeout(function() {
		// 1. 첫번째 무료로 시작하기 버튼 (gradient)
		addHoverEffect('#btn_start_free_1, *[id="btn_start_free_1"], *[style*="linear-gradient"]', {
			transform: 'translateY(-3px)',
			boxShadow: '0 12px 35px rgba(245, 158, 11, 0.6)',
			transition: 'all 0.3s ease'
		}, {
			transform: 'translateY(0px)',
			boxShadow: '0 8px 25px rgba(245, 158, 11, 0.4)',
			transition: 'all 0.3s ease'
		});
		
		// 2. 세번째 무료로 시작하기 버튼 (흰색)
		addHoverEffect('#btn_start_free_2, *[id="btn_start_free_2"], *[style*="background: white"]', {
			transform: 'translateY(-3px)',
			boxShadow: '0 12px 35px rgba(0,0,0,0.3)',
			background: '#f8f9fa',
			transition: 'all 0.3s ease'
		}, {
			transform: 'translateY(0px)',
			boxShadow: '0 8px 25px rgba(0,0,0,0.2)',
			background: 'white',
			transition: 'all 0.3s ease'
		});
		
		// 3. 기능 카드들
		for(let i = 1; i <= 6; i++) {
			addHoverEffect('#feature_card' + i + ', *[id="feature_card' + i + '"]', {
				transform: 'translateY(-10px)',
				boxShadow: '0 20px 50px rgba(0,0,0,0.15)',
				transition: 'all 0.3s ease'
			}, {
				transform: 'translateY(0px)',
				boxShadow: '0 10px 30px rgba(0,0,0,0.1)',
				transition: 'all 0.3s ease'
			});
		}
		
		// 4. 헤더 버튼들
		addHoverEffect('#btn_login, #btn_signup, *[id="btn_login"], *[id="btn_signup"]', {
			transform: 'translateY(-2px)',
			boxShadow: '0 4px 15px rgba(255, 184, 35, 0.4)',
			transition: 'all 0.3s ease'
		}, {
			transform: 'translateY(0px)',
			boxShadow: 'none',
			transition: 'all 0.3s ease'
		});
		
		// 5. 플로팅 카드들
		for(let i = 1; i <= 3; i++) {
			addHoverEffect('#Group_card' + i + ', *[id="Group_card' + i + '"]', {
				transform: 'translateY(-20px) scale(1.05)',
				boxShadow: '0 15px 40px rgba(245, 158, 11, 0.3)',
				animationPlayState: 'paused',
				transition: 'all 0.3s ease'
			}, {
				transform: '',
				boxShadow: '0 10px 30px rgba(245, 158, 11, 0.2)',
				animationPlayState: 'running',
				transition: 'all 0.3s ease'
			});
		}
		
	}, 500); // 500ms 후 실행하여 DOM이 완전히 로드되도록
};
]]></script>
